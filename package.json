{
  "name": "redux-mappers",
  "version": "1.0.4",
  "description": "Redux decorators to map from state to props or map actions to props",
  "main": "dist/index.js",
  "scripts": {
    "develop": "tsc --pretty --watch -p tsconfig.json",
    "build": "rm -rf ./dist && tsc -d --pretty -p tsconfig.json --sourceMap false --outdir ./dist",
    "test": "jest"
  },
  "sideEffects": false,
  "keywords": [
    "redux",
    "decorators",
    "map",
    "state",
    "props",
    "dispatch",
    "props"
  ],
  "author": "Hasan Oezdemir",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.4.0",
    "react-redux": "^5.0.7",
    "react-dom": "^16.4.0",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.10",
    "@types/enzyme-redux": "^0.2.0",
    "@types/jest": "^22.2.3",
    "@types/react": "^16.3.14",
    "@types/react-redux": "^6.0.0",
    "@types/redux": "^3.6.0",
    "@types/redux-mock-store": "0.0.13",
    "@types/redux-test-utils": "^0.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-redux": "^0.2.1",
    "jest": "^23.0.1",
    "jest-enzyme": "^6.0.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-mock-store": "^1.5.1",
    "redux-test-utils": "^0.2.2",
    "typescript": "^2.8.3"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/?(*.)spec.js"
    ],
    "verbose": true,
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js"
  }
}
